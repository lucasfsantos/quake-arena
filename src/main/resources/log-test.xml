<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" strict="true" monitorInterval="60">
	<Appenders>
		<Console name="CONSOLE">
			<PatternLayout pattern="%d{DEFAULT}] - %5p %c --- %m%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Root level="info">
			<AppenderRef ref="CONSOLE" />
		</Root>
	</Loggers>

</Configuration>